{
  "name": "@mastra/loggers",
  "version": "0.10.2",
  "description": "",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./file": {
      "import": {
        "types": "./dist/file/index.d.ts",
        "default": "./dist/file/index.js"
      },
      "require": {
        "types": "./dist/file/index.d.cts",
        "default": "./dist/file/index.cjs"
      }
    },
    "./upstash": {
      "import": {
        "types": "./dist/upstash/index.d.ts",
        "default": "./dist/upstash/index.js"
      },
      "require": {
        "types": "./dist/upstash/index.d.cts",
        "default": "./dist/upstash/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src/index.ts src/file/index.ts src/upstash/index.ts --format esm,cjs --experimental-dts --clean --treeshake=smallest --splitting",
    "build:watch": "pnpm build --watch",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "Elastic-2.0",
  "dependencies": {
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0"
