{
  "name": "@mastra/firecrawl",
  "version": "1.2.4-alpha.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --treeshake=smallest --splitting",
    "build:watch": "pnpm build --watch",
    "lint": "dts lint",
    "size": "size-limit",
    "start": "dts watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "clean": "rm -rf dist && rm -rf node_modules",
    "gen:zod:schema": "pnpx ts-to-zod  src/client/types.gen.ts src/client/zodSchema.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "size-limit": [
    {
      "path": "dist/mylib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mylib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@internal/lint": "workspace:*",
    "@jest/globals": "^29.7.0",
    "@mastra/core": "workspace:*",
    "@microsoft/api-extractor": "^7.52.8",
    "@rollup/plugin-image": "^3.0.3",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.19.0",
    "dts-cli": "^2.0.5",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "size-limit": "^11.2.0",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "keywords": [],
  "author": "",
  "license": "Elastic-2.0",
  "dependencies": {
    "@hey-api/client-fetch": "^0.3.4",
    "@mastra/rag": "workspace:^",
    "ts-to-zod": "^3.15.0",
    "zod": "^3.25.57"
  },
  "peerDependencies": {
    "@mastra/core": "^0.10.0-alpha.0"
  }
}
